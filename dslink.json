{
  "name": "dslink-java-alarm",
  "version": "0.1.3",
  "description": "Alarming Link",
  "license": "ISC",
  "author": {
    "name": "Aaron Hansen",
    "email": "aaron@comfortanalytics.com"
  },
  "main": "bin/dslink-java-alarm",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-alarm"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-alarm/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "alarming"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.alarm.jdbc.H2Main"
    }
  }
}
